{
  "name": "ssr",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react"
    ]
  },
  "husky": {
    "hooks": {
      "update": "yarn upgrade-interactive --latest",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,json,md,html,scss}": "prettier --write"
  },
  "scripts": {
    "start": "nodemon src",
    "start:dev": "webpack-dev-server --mode development",
    "debug": "node --inspect node_modules/webpack-dev-server/bin/webpack-dev-server --mode development",
    "build": "webpack --mode production",
    "serve": "webpack-dev-server --mode production --inline false --host 0.0.0.0",
    "update": "yarn upgrade-interactive --latest"
  },
  "dependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "babel-loader": "^8.2.2",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.5.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "serve": "^11.3.2",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.2",
    "webpack-node-externals": "^2.5.2",
    "webpack-sources": "^1.4.3"
  },
  "devDependencies": {
    "clean-webpack-plugin": "^3.0.0",
    "file-loader": "^6.2.0",
    "husky": "^4.3.8",
    "lint-staged": "^10.5.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "require-from-string": "^2.0.2",
    "serialize-javascript": "^4.0.0",
    "url-loader": "^4.1.1",
    "webpack-stats-plugin": "^0.3.2"
  }
}
